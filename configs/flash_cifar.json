{
  "output": "flash_cifar.log",
  "config_name": "flash_cifar",
  "memory_needed": 40,
  "command": "python train_flash.py",
  "args": [
    "--dataset=cifar",
    "--batch_size=256",
    "--learning_rate=5e-4",
    "--log_examples=20000",
