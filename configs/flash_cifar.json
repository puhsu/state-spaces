{
  "output": "flash_cifar.log",
  "config_name": "flash_cifar",
  "memory_needed": 40,
  "command": "python train_flash.py",
  "args": [
    "--dataset=cifar",
    "--batch_size=1024",
    "--learning_rate=0.05",
    "--log_examples=20000",
    "--log_file=flash.csv",
    "--lr_drop_factor=0.5",
    "--min_learning_rate=1e-6",
    "--pool=cls",
    "--num_layers=8",
    "--num_heads=8",
    "--hidden_size=128",
    "--embedding_dim=32",
    "--feedforward_size=64",
    "--dropout=0.1",
    "--attention_dropout=0.1",
    "--patience=10"
  ]
}